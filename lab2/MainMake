CC=g++
CFLAGS=-c -Wall -std=c++11

all: mainprog

mainprog: main.o processinfo.o processmanager.o
	$(CC) main.o processinfo.o processmanager.o -o mainprog

main.o: main.cpp
	$(CC) $(CFLAGS) main.cpp

processinfo.o: processinfo.cpp
	$(CC) $(CFLAGS) processinfo.cpp

processmanager.o: processmanager.cpp
	$(CC) $(CFLAGS) processmanager.cpp

clean:
	rm -rf *.o mainprog